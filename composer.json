{
  "name": "plaisio/mail-back",
  "description": "Plaisio: Mail - Back",
  "keywords": [
    "Plaisio",
    "Mail Message"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "phpmailer/phpmailer": "^6.7.1",
    "plaisio/blob-store": "^1.3.0",
    "plaisio/company-resolver": "^2.1.0",
    "plaisio/company-resolver-uni": "^2.1.1",
    "plaisio/console": "^3.0.0",
    "plaisio/dirs": "^1.3.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/mail-front": "^2.3.0",
    "psr/log": "^3.0.0",
    "setbased/exception": "^2.3.0",
    "symfony/console": "^6.2.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "plaisio/console-kernel": "^1.0.1",
    "setbased/php-stratum": "^6.3.1",
    "setbased/php-stratum-mysql": "^6.6.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Mail\\Command\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
